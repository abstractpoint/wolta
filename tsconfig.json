{
  "compilerOptions": {
    "lib": ["es2019"],
    "module": "commonjs",
    "target": "es2018",
    "noImplicitAny": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": "src",
    "paths": {
      "@/protocol/*": ["protocol/*"],
      "@/utils/*": ["utils/*"]
    }
  },
  "exclude": ["node_modules/**"]
}
